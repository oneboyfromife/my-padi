    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.oneboyfromife.mypadi"
    },
    "extra": {
      "eas": {
        "projectId": "c0609c99-2b65-44ec-bf84-814a3650f2eb"
      }
    }
  }
}
