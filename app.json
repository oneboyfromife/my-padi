{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "name": "My Padi",
    "slug": "dev-job",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/adaptive-icon.png",
    "userInterfaceStyle": "automatic",
